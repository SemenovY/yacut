[flake8]
 ignore = E501, E265, F811, PT001, DJ05, D100, D105, D104, W504, W292, A003, VNE003, N818, D107, D102
 max-line-length = 79
 paths = 
    ./yacut
 max-complexity = 10
 exclude = 
    tests
    migrations
 per-file-ignores =
    __init__.py: F401, E402, W292, A003, VNE003, N818, D107, D102
    error_handlers.py: D107, D102
    models.py: A003, VNE003
    api_views.py: E128
